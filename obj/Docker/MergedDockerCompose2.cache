name: dockercompose12386124739661903665
services:
  mongoclient:
    container_name: mongoclient
    depends_on:
      ticketdb:
        condition: service_started
    environment:
      MONGO_URL: mongodb://ticketdb:27017
    image: mongoclient/mongoclient
    networks:
      default: null
    ports:
    - mode: ingress
      target: 3000
      published: "3000"
      protocol: tcp
  ticketdb:
    container_name: ticketdb
    image: mongo
    networks:
      default: null
    ports:
    - mode: ingress
      target: 27017
      published: "27017"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: mongo_data
      target: /data/db
      volume: {}
  ticketing.api:
    build:
      context: C:\Users\User\Desktop\Project Moonshot\Tutorials\Automate-Test-and-Xray-Jira
      dockerfile: src/Services/Ticketing/Ticketing.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Ticketing.API
      target: base
    container_name: Ticketing.API
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: ""
    image: ticketingapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/src/../../isaac-ticket/Ticketing/src/Services/Ticketing/Ticketing.API/bin/Debug/net6.0/Ticketing.API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "5000"
      protocol: tcp
    - mode: ingress
      target: 443
      published: "5001"
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: C:\Users\User\Desktop\Project Moonshot\Tutorials\Automate-Test-and-Xray-Jira\src\Services\Ticketing\Ticketing.API
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\User\Desktop\Project Moonshot\Tutorials\Automate-Test-and-Xray-Jira
      target: /src
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose12386124739661903665_default
volumes:
  mongo_data:
    name: dockercompose12386124739661903665_mongo_data